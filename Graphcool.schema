type User @model {
  id: ID! @isUnique
  createdAt: DateTime!
  updatedAt: DateTime!

  email: String! @isUnique
  password: String!

  news: [News!]! @relation(name: "NewsAuthor")
}

type News @model {
  id: ID! @isUnique
  createdAt: DateTime!
  updatedAt: DateTime!

  title: String!
  url: String!

  author: User! @relation(name: "NewsAuthor")
}
